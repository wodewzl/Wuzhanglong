                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="@color/C3"
                android:gravity="center"
                android:paddingLeft="40dp"
                android:paddingRight="40dp" >

                <TextView
                    android:id="@+id/last_month"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/corners_tab_trade_left_normal"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:maxWidth="150dp"
                    android:paddingLeft="10dp"
                    android:singleLine="true"
                    android:text="上月"
                    android:textColor="@color/C5" />

                <View
                    android:layout_width="0.5dp"
                    android:layout_height="fill_parent"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:background="#dddddd" />

                <TextView
                    android:id="@+id/this_month"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/corners_tab_center_select"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:maxWidth="150dp"
                    android:paddingLeft="10dp"
                    android:singleLine="true"
                    android:text="本月"
                    android:textColor="@color/C1" />

                <View
                    android:layout_width="0.5dp"
                    android:layout_height="fill_parent"
              android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:background="#dddddd" />

                <TextView
                    android:id="@+id/this_season"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:background="@drawable/corners_tab_trade_right_normal"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:maxWidth="150dp"
                    android:paddingLeft="10dp"
                    android:singleLine="true"
                    android:text="本季度"
                    android:textColor="@color/C5" />
            </LinearLayout>

            <LinearLayout
